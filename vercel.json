{
  "rewrites": [
    {
      "source": "/:page.html",
      "destination": "/:page"
    },
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/contact",
      "destination": "/contact.html"
    },
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/(.*)",
      "destination": "/404.html"
    }
  ]
}
